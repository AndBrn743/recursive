    <sources>
        <source filename="../Recursive Sans-Casual B Slanted.ufo" familyname="Recursive Sans" stylename="Casual B Slanted">
            <location>
                <dimension name="Proportion" xvalue="1" />
                <dimension name="Slant" xvalue="14" />
                <dimension name="Italic" xvalue="0.500000" />
            </location>
        </source>
        <source filename="../Recursive Sans-Casual B.ufo" familyname="Recursive Sans" stylename="Casual B">
            <location>
                <dimension name="Proportion" xvalue="1" />
                <dimension name="Slant" xvalue="0" />
                <dimension name="Italic" xvalue="0.500000" />
            </location>
        </source>
        <source filename="../../mono/Recursive Mono-Casual B Slanted.ufo" familyname="Recursive Mono" stylename="Casual B Slanted">
            <location>
                <dimension name="Proportion" xvalue="0" />
                <dimension name="Slant" xvalue="14" />
                <dimension name="Italic" xvalue="0.500000" />
            </location>
        </source>
        <source filename="../../mono/Recursive Mono-Casual B.ufo" familyname="Recursive Mono" stylename="Casual B">
            <location>
                <dimension name="Proportion" xvalue="0" />
                <dimension name="Slant" xvalue="0" />
                <dimension name="Italic" xvalue="0.500000" />
            </location>
        </source>
    </sources>
    <lib>
        <dict>
            <key>com.letterror.skateboard.interactionSources</key>
            <dict>
                <key>horizontal</key>
                <array>
                    <string>Slant</string>
                </array>
                <key>ignore</key>
                <array>
                    <string>Proportion</string>
                    <string>Italic</string>
                </array>
                <key>vertical</key>
                <array />
            </dict>
            <key>com.letterror.skateboard.previewLocation</key>
            <dict>
                <key>Italic</key>
                <real>0.5</real>
                <key>Proportion</key>
                <real>0.0</real>
                <key>Slant</key>
                <real>0.0</real>
            </dict>
            <key>com.letterror.skateboard.previewText</key>
            <string>ilfrg/i.mono/l.mono/f.mono/r.mono/g.mono</string>
        </dict>
    </lib>
</designspace>