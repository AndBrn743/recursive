<?xml version="1.0" encoding="UTF-8" ?>
<designspace format="4.0">
    <axes>
        <axis tag="PROP" name="Proportion" minimum="0" maximum="1" default="0">
            <labelname xml:lang="en">Proportion</labelname>
        </axis>
        <axis tag="slnt" name="Slant" minimum="0" maximum="14" default="0">
            <labelname xml:lang="en">Slant</labelname>
        </axis>
        <axis tag="ital" name="Italic" minimum="0" maximum="1" default="0.500000">
            <labelname xml:lang="en">Italic</labelname>
        </axis>
    </axes>
    <rules>


        <rule name="mono">
            <conditionset>
                <condition name="Proportion" minimum="0" maximum="0.510000" />
            </conditionset>
            <sub name="f" with="f.mono" />
            <sub name="g" with="g.mono" />
            <sub name="i" with="i.mono" />
            <sub name="l" with="l.mono" />
            <sub name="r" with="r.mono" />
        </rule>
        <rule name="sans">
            <conditionset>
                <condition name="Proportion" minimum="0.510000" maximum="1" />
            </conditionset>
            <sub name="l" with="l.sans" />
        </rule>

