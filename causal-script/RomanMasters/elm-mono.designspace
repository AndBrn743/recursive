            <info copy="1" />
            <location>
                <dimension name="optical" xvalue="50" />
                <dimension name="weight" xvalue="500" />
            </location>
        </source>
    </sources>
    <instances>
        <instance familyname="Elm Text" filename="instances/ElmText-ExtraLight.ufo" postscriptfontname="ElmText-ExtraLight" stylename="Extra Light">
            <location>
                <dimension name="optical" xvalue="10" />
                <dimension name="weight" xvalue="100" />
            </location>
            <kerning />
            <info />
        </instance>
    </instances>
</designspace>
